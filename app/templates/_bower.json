{
    "name": "<%=appName%>",
    "description": "<%=appDescription%>",
    "version": "<%=appVersion%>",
    "ignore" : [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {
        "requirejs": "~2.1.14",
        "underscore" : "~1.6.0",
        "backbone": "~1.1.2",
        "jquery": "1.10.2"
    },
    "resolutions" : {
        "jquery" : "1.10.2"
    },
    "install" : {
        "path" : {
            "js" : "assets/js/vendor",
            "css": "assets/css/vendor"
        },
        "sources" : {
            "jquery": [
                "bower_components/jquery/jquery.min.js"
            ]
        }
    }
}
