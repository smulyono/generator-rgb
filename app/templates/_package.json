{
    "name": "<%=appName%>",
    "description": "<%=appDescription%>",
    "version": "<%=appVersion%>",
    "author": {
        "name": "<%=author%>",
        "email": "<%=authorEmail%>"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "scripts": {
        "prestart": "npm install",
        "postinstall": "./node_modules/.bin/bower cache clean && ./node_modules/.bin/bower install && ./node_modules/.bin/bower-installer",
        "start": "./node_modules/.bin/grunt",
        "build": "./node_modules/.bin/grunt build",
        "build_component" : "./node_modules/.bin/grunt build --build_dir=dist",
        "start_live": "./node_modules/.bin/grunt connect:live",
        "test" : "./node_modules/.bin/grunt test"
    },
    "keywords": ["backbone", "requirejs", "grunt", "rgb"],
    "dependencies": {
        "bower": "1.3.12",
        "bower-installer": "0.8.4",
        "grunt": "~0.4.5",
        "grunt-browser-sync": "^2.1.3",
        "grunt-cli": "~0.1.13",
        "grunt-compare-size": "^0.4.0",
        "grunt-contrib-clean": "~0.6.0",
        "grunt-contrib-concat": "~0.5.1",
        "grunt-contrib-connect": "~0.9.0",
        "grunt-contrib-cssmin": "~0.12.2",
        "grunt-contrib-less": "~1.0.1",
        "grunt-contrib-requirejs": "~0.4.4",
        "grunt-contrib-uglify": "~0.8.0",
        "grunt-contrib-watch": "~0.6.1",
        "grunt-jsdoc": "~0.5.8",
        "grunt-karma": "~0.10.1",
        "grunt-usemin": "^3.0.0",
        "karma": "~0.12.31",
        "karma-chrome-launcher": "~0.1.7",
        "karma-coverage": "~0.4.2",
        "karma-jasmine": "~0.3.5",
        "karma-jasmine-ajax": "~0.1.12",
        "karma-junit-reporter": "~0.2.2",
        "karma-mocha-reporter": "~1.0.2",
        "karma-phantomjs-launcher": "~0.2.0",
        "karma-requirejs": "~0.2.2",
        "karma-script-launcher": "~0.1.0",
        "load-grunt-tasks": "~3.1.0",
        "requirejs": "2.1.16",
        "time-grunt": "^1.1.0"
    },
    "peerDependencies" : {
        "karma": "~0.12.31",
        "jasmine-core" : "~2.3.4",
        "requirejs": "2.1.16",
        "source-map": "^0.4.2"
    }
}